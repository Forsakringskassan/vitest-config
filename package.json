{
  "name": "@forsakringskassan/vitest-config",
  "version": "1.1.0",
  "description": "Försäkringskassan shareable config for Vitest",
  "keywords": [
    "vitest"
  ],
  "homepage": "https://github.com/Forsakringskassan/vitest-config#readme",
  "bugs": {
    "url": "https://github.com/Forsakringskassan/vitest-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/vitest-config.git"
  },
  "license": "MIT",
  "author": "Föräkringskassan",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vitest": "vitest.mjs"
  },
  "files": [
    "dist/",
    "vitest.mjs"
  ],
  "scripts": {
    "prebuild": "tsc",
    "build": "node build.mjs",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "lint": "run-s prettier:check eslint",
    "prepack": "release-prepack --bundle",
    "postpack": "release-postpack",
    "prepublishOnly": "release-prepublish --bundle",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postpublish": "release-postpublish",
    "pretest": "npm run lint",
    "test": "true"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  },
  "dependencies": {
    "@vitest/coverage-v8": "4.0.18",
    "vitest": "4.0.18"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "3.3.3",
    "@forsakringskassan/eslint-config": "13.3.0",
    "@forsakringskassan/prettier-config": "3.3.4",
    "@html-validate/release-scripts": "7.1.0",
    "@tsconfig/node22": "22.0.5",
    "deepmerge": "4.3.1",
    "esbuild": "0.27.3",
    "npm-run-all2": "8.0.4",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">= 22.12.0",
    "npm": ">= 10.0.0"
